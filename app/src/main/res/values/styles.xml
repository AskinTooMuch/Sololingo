<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="tabActive" parent="tab">
        <item name="android:textColor">@color/font</item>
        <item name="android:background">@color/yellow_60</item>
    </style>

    <style name="tab">
        <item name="android:textAlignment">center</item>
        <item name="android:textColor">@color/fontTrans</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textAllCaps">true</item>
    </style>

    <style name="insUsername">
        <item name="android:layout_width">310dp</item>
        <item name="android:layout_height">51dp</item>
        <item name="android:background">#74ECDFDF</item>
        <item name="android:backgroundTint">#75FFFFFF</item>
        <item name="android:ems">10</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:textColorHint">@color/white</item>
    </style>

    <style name="registerButton">
        <item name="android:layout_width">409dp</item>
        <item name="android:layout_height">66dp</item>
        <item name="android:backgroundTint">#5BECE1E1</item>
        <item name="android:text">@string/Donnot_have_a_account</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">14sp</item>
        <item name="layout_constraintHorizontal_bias">0.0</item>
        <item name="layout_constraintVertical_bias">1.0</item>
    </style>
</resources>